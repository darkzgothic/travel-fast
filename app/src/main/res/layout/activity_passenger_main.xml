<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_passenger_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.zahidhasan.travelfast.passenger.activity.PassengerMainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@color/colorPrimary"
            android:orientation="horizontal">

            <com.robertlevonyan.views.customfloatingactionbutton.CustomFloatingActionButton
                android:id="@+id/custom_fab"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="80dp"
                app:fabType="square"
                app:fabText="Home"
                app:fabColor="@color/colorPrimary" />

            <com.robertlevonyan.views.customfloatingactionbutton.CustomFloatingActionButton
                android:id="@+id/custom_fab2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="80dp"
                app:fabType="square"
                app:fabText="Invite"
                app:fabColor="@color/colorPrimary" />

        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>
